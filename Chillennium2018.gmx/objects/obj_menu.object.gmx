            <string>gamepad = global.player;
if(gamepad != -1) {
    if(gamepad_axis_value(gamepad, gp_axislv) &gt; 0.5 &amp;&amp; move) {
        audio_play_sound(snd_menu_move, 10, false);
        select = (select + 1) mod 4;
        instance_destroy(obj_simple_chameleon);
        instance_create(x, y, obj_simple_chameleon);
        move = false;
    }
    else if(gamepad_axis_value(gamepad, gp_axislv) &lt; -0.5 &amp;&amp; move) {
        audio_play_sound(snd_menu_move, 10, false);
        select = select - 1;
        if(select &lt; 0) {
            select = 3;
        }
        move = false;
        instance_destroy(obj_simple_chameleon);
        instance_create(x, y, obj_simple_chameleon);
    }
    else if(gamepad_axis_value(gamepad, gp_axislv) &gt;= -0.5 &amp;&amp; gamepad_axis_value(gamepad, gp_axislv) &lt;= 0.5) {
        move = true;
    }
    
    if(gamepad_button_check_pressed(gamepad, gp_face1)) {
        audio_play_sound(snd_tongue_out, 10, false);
        alarm[0] = room_speed * 0.6;
    }
}
</string>
