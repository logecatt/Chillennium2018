with obj_chameleon {
    if(gamepad == global.player2) {
        eye.image_angle += 180;
    }
    switch(ind){
        case 0: 
            type = obj_swarm_fire; 
            head.prev_sprite_index = spr_head_fire
            head.target_sprite_index = spr_head_fire
            target_image_index = 2
            break;
        case 1: 
            type = obj_swarm_water; 
            head.prev_sprite_index = spr_head_water
            head.target_sprite_index = spr_head_water
            target_image_index = 1
            break;
        case 2: 
            type = obj_swarm_grass; 
            head.prev_sprite_index = spr_head_grass
            head.target_sprite_index = spr_head_grass
            target_image_index = 0
            break;
    }
}</code>
